{
  "name": "duck-decoy",
  "version": "0.1.0",
  "description": "Simple HTTP mocks and fakes",
  "author": "Sven Johansson",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "npm run typecheck && vite build",
    "ci:test": "vitest run --reporter=dot --no-color",
    "lint": "eslint .",
    "test": "vitest run --reporter verbose",
    "test:unit": "vitest run test/unit --reporter verbose",
    "test:e2e": "vitest run test/e2e --reporter verbose",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@>=10.8.2",
  "dependencies": {
    "@types/node": "^24.2.0",
    "fastify-type-provider-zod": "^5.0.1",
    "got": "^14.4.7",
    "lokijs": "^1.5.12",
    "luxon": "^3.6.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "axios": "^1.11.0",
    "prettier": "^3.6.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
