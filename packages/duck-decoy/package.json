{
  "name": "duck-decoy",
  "version": "0.1.6",
  "description": "Simple HTTP mocks and fakes",
  "author": "Sven Johansson",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "pnpm typecheck && tsup",
    "ci:test": "vitest run --reporter=dot --no-color",
    "lint": "pnpm exec eslint .",
    "prepack": "pnpm build",
    "test": "pnpm exec vitest run --reporter verbose",
    "test:unit": "pnpm exec vitest run test/unit --reporter verbose",
    "test:e2e": "pnpm exec vitest run test/e2e --reporter verbose",
    "test:watch": "pnpm exec vitest",
    "typecheck": "pnpm exec tsc --noEmit"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@>=10.8.2",
  "dependencies": {
    "luxon": "^3.6.1"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.21.3",
    "prettier": "^3.6.2",
    "tsup": "^8.5.1",
    "typescript-eslint": "^8.39.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
