{
  "name": "@duck-decoy/collections-knex",
  "version": "0.2.0",
  "type": "module",
  "description": "Knex-backed RecordCollection for Duck Decoy",
  "license": "MIT",
  "author": "Sven Johansson",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "pnpm typecheck && tsup",
    "ci:test": "vitest run --reporter=dot --no-color",
    "lint": "pnpm exec eslint .",
    "prepack": "pnpm build",
    "test": "vitest run --reporter verbose",
    "typecheck": "pnpm exec tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "duck-decoy": "workspace:*",
    "mssql": "^12.2.0",
    "tsup": "^8.5.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "knex": "^3.1.0",
    "duck-decoy": "^0.2.0"
  }
}
