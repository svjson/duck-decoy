  "description": "Simple HTTP mocks and fakes - running on Fastify",
  "type": "module",
  "author": "Sven Johansson",
  "license": "MIT",
  "files": ["dist", "README.md", "LICENSE"],
  "publishConfig": { "access": "public" },
