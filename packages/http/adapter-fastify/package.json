{
  "name": "@duck-decoy/fastify",
  "version": "0.1.5",
  "type": "module",
  "description": "Simple HTTP mocks and fakes - running on Fastify",
  "license": "MIT",
  "author": "Sven Johansson",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "npm run typecheck && vite build",
    "lint": "npx eslint .",
    "prepack": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "duck-decoy": "^0.1.4",
    "fastify": "^5.4.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/session": "^11.1.0"
  },
  "devDependencies": {
    "duck-decoy": "*"
  }
}
