{
  "name": "@duck-decoy/swagger",
  "version": "0.1.2",
  "description": "Swagger UI and OpenAPI generation for Duck Decoy",
  "author": "Sven Johansson",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "pnpm typecheck && tsup",
    "ci:test": "vitest run --reporter=dot --no-color",
    "lint": "pnpm exec eslint .",
    "prepack": "pnpm build",
    "test": "vitest run --reporter verbose",
    "test:unit": "vitest run test/unit --reporter verbose",
    "test:e2e": "vitest run test/e2e --reporter verbose",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@>=10.8.2",
  "peerDependencies": {
    "duck-decoy": "^0.1.5"
  },
  "dependencies": {
    "openapi-types": "^12.1.3",
    "swagger-ui-dist": "^5.28.1"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "@types/swagger-ui-dist": "^3.30.6",
    "duck-decoy": "workspace:*",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.21.3",
    "prettier": "^3.6.2",
    "tsup": "^8.5.1",
    "typescript-eslint": "^8.39.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
